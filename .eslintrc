{
  'ecmaFeatures': {
    'jsx': true,
    'modules': true
  },
  'env': {
    'browser': true,
    'node': true
  },
  'parser': 'babel-eslint',
  'rules': {
    'babel/generator-star-spacing': 1,
    'babel/new-cap': 0,
    'babel/object-shorthand': 1,
    'babel/arrow-parens': [2, 'as-needed'],
    'babel/no-await-in-loop': 1,
    'react/jsx-uses-react': 2,
    'react/jsx-uses-vars': 2,
    'arrow-spacing': 2,
    'comma-dangle': [2, 'never'],
    'constructor-super': 2,
    'eqeqeq': 2,
    'quotes': [2, 'single'],
    'no-cond-assign': 2,
    'no-console': 1,
    'no-constant-condition': 2,
    'no-control-regex': 2,
    'no-debugger': 2,
    'no-dupe-args': 2,
    'no-dupe-keys': 2,
    'no-duplicate-case': 2,
    'no-empty': 2,
    'no-empty-character-class': 2,
    'no-ex-assign': 2,
    'no-extra-boolean-cast': 2,
    'no-extra-semi': 2,
    'no-func-assign': 2,
    'no-inner-declarations': 2,
    'no-invalid-regexp': 2,
    'no-irregular-whitespace': 2,
    'no-negated-in-lhs': 2,
    'no-obj-calls': 2,
    'no-regex-spaces': 2,
    'no-sparse-arrays': 2,
    'no-unexpected-multiline': 2,
    'no-unreachable': 2,
    'use-isnan': 2,
    'valid-typeof': 2,
    'strict': [2, 'never'],
    'no-empty-pattern': 2,
    'no-multi-spaces': 2,
    'no-octal': 2,
    'no-redeclare': 2,
    'no-self-assign': 2,
    'no-unused-labels': 2,
    'no-delete-var': 2,
    'no-undef': 2,
    'no-unused-vars': 1,
    'block-spacing': 1,
    'comma-spacing': 1,
    'computed-property-spacing': 1,
    'eol-last': 1,
    'jsx-quotes': [2, 'prefer-double'],
    'keyword-spacing': 2,
    'no-mixed-spaces-and-tabs': 2,
    'no-trailing-spaces': 1,
    'semi': ['error', 'always'],
    'semi-spacing': [2, {'before': false, 'after': true}],
    'space-before-blocks': 2,
    'space-in-parens': 2,
    'space-infix-ops': 2,
    'space-unary-ops': 2,
    'template-curly-spacing': 2
  },
  'plugins': [
    'babel',
    'react'
  ],
  'globals': {
    'R1SOFT': true,
    'describe': true,
    'it': true,
    'beforeEach': true,
    'afterEach': true
  }
}
